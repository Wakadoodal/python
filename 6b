function validateUSN()
{
    var userInput=document.getElementById('usn');
    var usn=userInput.value.trim();
    var pattern = /^[1-4][A-Z]{2}\d{2}[A-Z]{2}\d{3}$/;
    if(pattern.test(usn))
    {
        alert("it is valid");
    }
    else{
        alert("this is not valid");
    }
    userInput.value="";
}
